{
    "manifest_version": 3,
    "name": "NotePad",
    "version": "1.0",
    "description": "Create quick notes and download them",
    "permissions": ["tabs", "storage"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }
  
